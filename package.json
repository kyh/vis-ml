{
  "name": "vis-ml",
  "description": "A visual introduction to machine learning topics",
  "version": "0.3.0",
  "author": "Kaiyu Hsu",
  "scripts": {
    "build": "npm run clean && parcel build src/*.html",
    "clean": "rimraf dist",
    "dev": "parcel serve src/*.html --open",
    "format": "prettier --write src",
    "postinstall": "husky install"
  },
  "targets": {
    "browser": {
      "engines": {
        "browsers": [
          "last 2 versions",
          "not edge < 76",
          "not firefox < 67"
        ]
      }
    }
  },
  "dependencies": {
    "d3": "^7.3.0",
    "intersection-observer": "^0.12.0",
    "lodash": "^4.17.21",
    "scrollama": "^3.0.3",
    "stickyfilljs": "^2.1.0"
  },
  "devDependencies": {
    "husky": "^7.0.4",
    "parcel": "^2.2.1",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2"
  }
}
