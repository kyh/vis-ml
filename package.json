{
  "name": "vis-ml",
  "description": "A visual introduction to machine learning topics",
  "version": "0.1.0",
  "main": "src/index.html",
  "author": "Kaiyu Hsu",
  "scripts": {
    "build": "npm run clean && parcel build src/*.html",
    "clean": "rimraf dist",
    "dev": "parcel serve src/*.html --open",
    "format": "prettier --write src",
    "postinstall": "husky install"
  },
  "browser": "dist/index.html",
  "targets": {
    "browser": {
      "engines": {
        "browsers": [
          "last 2 versions",
          "not edge < 76",
          "not firefox < 67"
        ]
      }
    }
  },
  "dependencies": {
    "d3": "^5.15.0",
    "intersection-observer": "^0.7.0",
    "scrollama": "^2.2.0",
    "stickyfilljs": "^2.1.0"
  },
  "devDependencies": {
    "husky": "^6.0.0",
    "parcel": "next",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "rimraf": "^3.0.2"
  }
}
